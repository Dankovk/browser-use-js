{
	"name": "browser-use-node",
	"version": "0.1.9",
	"description": "Browser automation powered by LLMs in JavaScript",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build:binary": "bun run clean && bun --bun build --compile src/index.ts --compile --outfile=dist/browser-use-node",
		"build:examples-binary": "bun run clean && bun run scripts/compile-examples.ts",
		"build": "bun run clean && bun build ./src/index.ts --outdir=dist --target=bun --sourcemap",
		"clean": "rm -rf dist",
		"build:examples": "bun run clean && bun build ./examples/*.ts --outdir=dist/examples --target=node && tsc --emitDeclarationOnly --outDir dist",
		"test": "bun test",
		"lint": "bun lint",
		"format": "bun prettier --write src/**/*.ts",
		"prepare": "bun --bun run build",
		"example:amazon": "bun --bun run examples/amazon-search.ts",
		"example:multi-tab": "bun --bun run examples/multi-tab.ts",
		"example:simple": "bun --bun run examples/simple.ts",
		"test:smoke": "bun test tests/smoke/smoke.test.ts",
		"deploy:staging": "node scripts/deploy.js staging",
		"deploy:production": "node scripts/deploy.js production",
		"type-check": "bun tsc --noEmit",
		"docs:dev": "cd docs && bun run start",
		"docs:build": "cd docs && bun run build",
		"docs:serve": "cd docs && bun run serve",
		"docs:clear": "cd docs && bun run clear",
		"docs:generate-api": "typedoc --out docs/docs/api src/index.ts --skipErrorChecking --excludeExternals --excludePrivate --plugin typedoc-plugin-markdown",
		"docs": "bun run docs:generate-api && bun run docs:build && bun run docs:serve"
	},
	"files": ["dist", "README.md", "LICENSE"],
	"keywords": [
		"browser",
		"automation",
		"llm",
		"ai",
		"playwright",
		"node",
		"browser-use",
		"browser-use-node",
		"browser-use-js"
	],
	"author": "Your Name",
	"license": "MIT",
	"dependencies": {
		"cheerio": "^1.0.0-rc.12",
		"chromium-bidi": "^0.4.32",
		"dotenv": "^16.4.7",
		"gif-encoder-2": "^1.0.5",
		"gifencoder": "^2.0.1",
		"langchain": "^0.0.200",
		"playwright": "^1.40.1",
		"playwright-core": "^1.40.1",
		"posthog-node": "^3.1.3",
		"sharp": "^0.33.2",
		"uuid": "^9.0.1",
		"zod": "^3.22.4",
		"limiter": "^2.1.0",
		"xss": "^1.0.14",
		"chai": "^4.3.7"
	},
	"devDependencies": {
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.6",
		"@types/uuid": "^9.0.7",
		"@types/cheerio": "^0.22.35",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"eslint": "^8.56.0",
		"jest": "^29.7.0",
		"prettier": "^3.1.1",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.2",
		"typescript": "^5.3.3",
		"@types/chai": "^4.3.5",
		"semantic-release": "^21.0.5"
	}
}
