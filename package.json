{
	"name": "browser-use-test",
	"version": "1.0.0",
	"description": "Browser automation with enhanced capabilities",
	"main": "dist/index.js",
	"scripts": {
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"test:browser": "jest tests/browser",
		"test:controller": "jest tests/controller",
		"test:dom": "jest tests/dom",
		"test:agent": "jest tests/agent",
		"test:e2e": "jest tests/e2e"
	},
	"dependencies": {
		"playwright": "^1.42.1",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.24",
		"@types/uuid": "^9.0.8",
		"jest": "^29.7.0",
		"ts-jest": "^29.1.2",
		"typescript": "^5.3.3"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"testMatch": [
			"**/tests/**/*.test.ts"
		],
		"moduleFileExtensions": [
			"ts",
			"js",
			"json"
		],
		"collectCoverageFrom": [
			"src/**/*.ts"
		],
		"coverageReporters": [
			"text",
			"lcov",
			"html"
		],
		"setupFilesAfterEnv": [
			"<rootDir>/tests/setup.ts"
		]
	}
}
