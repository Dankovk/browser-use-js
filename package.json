{
	"name": "browser-use-node",
	"version": "0.1.9",
	"description": "Browser automation powered by LLMs in JavaScript",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"type": "module",
	"scripts": {
		"build": "bun build ./src/index.ts --outdir=dist --target=bun",
		"clean": "rm -rf dist",
		"test": "bun test",
		"lint": "bun run eslint src/**/*.ts",
		"format": "bun x prettier --write src/**/*.ts",
		"example:amazon": "bun run examples/amazon-search.ts",
		"example:multi-tab": "bun run examples/multi-tab.ts",
		"example:simple": "bun run examples/simple.ts",
		"test:smoke": "bun test tests/smoke/smoke.test.ts",
		"type-check": "bun run tsc --noEmit",
		"docs:dev": "cd docs && bun run start",
		"docs:build": "cd docs && bun run build",
		"docs:serve": "cd docs && bun run serve",
		"docs:clear": "cd docs && bun run clear",
		"docs:generate-api": "bunx typedoc --out docs/docs/api src/index.ts --skipErrorChecking --excludeExternals --excludePrivate --plugin typedoc-plugin-markdown",
		"docs": "bun run docs:generate-api && bun run docs:build && bun run docs:serve"
	},
	"files": [
		"dist",
		"README.md",
		"LICENSE"
	],
	"keywords": [
		"browser",
		"automation",
		"llm",
		"ai",
		"playwright",
		"node",
		"browser-use",
		"browser-use-node",
		"browser-use-js"
	],
	"author": "Your Name",
	"license": "MIT",
	"dependencies": {
		"@langchain/openai": "^0.0.10",
		"@types/jsdom": "^21.1.7",
		"@types/turndown": "^5.0.5",
		"@types/winston": "^2.4.4",
		"axios": "^1.7.9",
		"cheerio": "^1.0.0-rc.12",
		"chromium-bidi": "^0.4.32",
		"dotenv": "^16.4.7",
		"jsdom": "^25.0.1",
		"langchain": "^0.1.0",
		"limiter": "^2.1.0",
		"playwright": "^1.49.1",
		"playwright-core": "^1.40.1",
		"posthog-node": "^3.1.3",
		"traceback": "^0.3.1",
		"turndown": "^7.2.0",
		"uuid": "^9.0.1",
		"winston": "^3.17.0",
		"xss": "^1.0.14",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/node": "^20.10.6",
		"@types/uuid": "^9.0.7",
		"@typescript-eslint/eslint-plugin": "^6.16.0",
		"@typescript-eslint/parser": "^6.16.0",
		"eslint": "^8.56.0",
		"prettier": "^3.1.1",
		"typescript": "^5.3.3"
	}
}
